        LED_set(gakufu[cnt][0] , gakufu[cnt][0]);
        LED_set(gakufu[cnt][1] , gakufu[cnt][1]);
        LED_set(gakufu[cnt][2] , gakufu[cnt][2]);
        LED_set(gakufu[cnt][3] , gakufu[cnt][3]);
        delay(50);

        servo_write(gakufu[cnt][0], offset[gakufu[cnt][0]]);
        servo_write(gakufu[cnt][1], offset[gakufu[cnt][1]]);
        servo_write(gakufu[cnt][2], offset[gakufu[cnt][2]]);
        servo_write(gakufu[cnt][3], offset[gakufu[cnt][3]]);

        pixels.clear();   //LED 全部消灯
        pixels.show();
        delay(50);
      }
    }
    cnt += 1;
    Serial.print("cnt=");
    Serial.println(cnt); // 改行


    M5.update();
    if(M5.BtnA.wasPressed()) {  // 途中停止
      gakufu[cnt][0] = 99;      // 途中停止 
    }                           // 途中停止

    if(gakufu[cnt][0] >= 99){
      mode = 0;
      Serial.print("mode=");
      Serial.println(mode); // 改行
      delay(200);
      pixels.clear();   //LED 全部消灯
      pixels.show();

/*
      if(sel_menu == 3){
        for(i = 0 ; i < 32 ; i++){
          delay(20);
          servo_write(i, 90);
        }
      }
*/
    }
  }
}



void servo_write(int ch, int ang){  // 動かすサーボチャンネルと角度を指定
  ang = map(ang, 0, 180, SERVOMIN, SERVOMAX); //角度(0-180)をPWMのパルス幅(150-500)に変換

  if(ch < 16){
    pwm.setPWM(ch, 0, ang);
  }
  else{
    pwm2.setPWM(ch-16, 0, ang);
  }
}


